{
  "name": "webpack-vue-seed",
  "version": "1.0.0",
  "description": "a vue webpack template",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --progress --colors --inline",
    "build": "webpack --progress --colors",
    "dev": "npm run build && npm run start",
    "release": "webpack -p --config webpack.prod.config.js",
    "lint": "eslint 'src/**/*.@(js|jsx|es)'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bs": "cd dist && browser-sync start --server --files '**/*.html,**/*.js,**/*.css' --directory"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ygtzz/webpack-vue-seed.git"
  },
  "keywords": [
    "vue",
    "webpack"
  ],
  "author": "ygtzz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ygtzz/webpack-vue-seed/issues"
  },
  "homepage": "https://github.com/ygtzz/webpack-vue-seed#readme",
  "dependencies": {
    "vue": "^1.0.26",
    "vue-router": "^0.7.13",
    "vuex": "^1.0.0-rc.2",
    "vuex-router-sync": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-preset-es2015": "^6.18.0",
    "clean-webpack-plugin": "^0.1.13",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.0",
    "open-browser-webpack-plugin": "0.0.2",
    "path": "^0.12.7",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
